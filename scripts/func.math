func error(var a; var b; var c){
    if( a+b > c){
        print("error a+b > c!");
    }
}

func sumator(var a; var b){
    var c = (a+b)*10;
    error(a,b,c);
    for(var i = 1; i != 5; i++){
        a = a*i;
        if(a < 10){
            a = a + 5;
            continue;
        }
        else{
            break;
        }
    }
    return c-(a+b);
}

print(1,2,3);

var tonk = sumator(10,10);

print(" tonk = %tonk% ");

var b = 10;
var t = 1;

print(sumator(t,b), "");